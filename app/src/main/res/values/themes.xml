<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.FitQuest" parent="android:Theme.Material.NoActionBar">
        <item name="android:colorBackground">@color/darker</item>
    </style>


    <style name="Theme.FitQuestSplashScreen" parent="Theme.SplashScreen">
